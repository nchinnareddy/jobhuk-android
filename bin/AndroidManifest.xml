    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/jobhuk"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.startupsourcing.jobhuk.JobHuk_Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.startupsourcing.jobhuk.JobsDescription"
            android:label="@string/title_activity_hello" >
        </activity>
        <activity
            android:name="com.startupsourcing.jobhuk.Refer"
            android:label="@string/title_activity_refer" >
        </activity>
        <activity
            android:name="com.startupsourcing.jobhuk.Login"
            android:label="@string/title_activity_login" >
        </activity>
    </application>

</manifest>
